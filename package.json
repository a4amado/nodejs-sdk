{
  "name": "webdock",
  "version": "1.0.2",
  "description": "NodeJS SDK Library / Wrapper for the Webdock API",
  "scripts": {
    "release": "release-it",
    "build": "tsc",
    "generate-client": "node generate.js",
    "develop": "ts-node-dev --transpile-only --no-notify",
    "test": "jest",
    "prepublishOnly": "npm run generate-client && npm run build && cp -r ./dist/* . && rm -rf ./dist",
    "postpublish": "git clean -fd",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.20",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "simple-git-hooks": "^2.8.1",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "webdock-io",
    "webdock"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write"
    ]
  },
  "author": "Alex0007 <Alex0007lolpvp@gmail.com>",
  "license": "ISC",
  "repository": "github:webdock-io/nodejs-sdk"
}