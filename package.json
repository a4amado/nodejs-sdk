{
  "name": "webdock",
  "version": "0.1.7",
  "description": "NodeJS SDK Library / Wrapper for the Webdock API",
  "scripts": {
    "release": "release-it",
    "build": "tsc",
    "generate-client": "node generate.js",
    "develop": "ts-node-dev --transpile-only --no-notify",
    "test": "jest",
    "prepublishOnly": "npm run generate-client && npm run build && cp -r ./dist/* . && rm -rf ./dist",
    "postpublish": "git clean -fd"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.19",
    "openapi-typescript-codegen": "^0.4.8",
    "prettier": "^2.0.5",
    "release-it": "^13.6.6",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "webdock-io",
    "webdock"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Alex0007 <Alex0007lolpvp@gmail.com>",
  "license": "ISC"
}
