{
  "name": "webdock",
  "version": "0.0.17",
  "description": "NodeJS SDK Library / Wrapper for the Webdock API",
  "scripts": {
    "build": "tsc",
    "preprocess-schema": "node preprocess.js",
    "generate-client": "openapi --input ./webdock.json --output generated --client node-fetch",
    "develop": "ts-node-dev --transpile-only --no-notify",
    "prepublishOnly": "npm run generate-client && npm run build && cp -r ./dist/* . && rm -rf ./dist",
    "postpublish": "git clean -fd"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "openapi-typescript-codegen": "^0.4.8",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "webdock-io",
    "webdock"
  ],
  "author": "Alex0007 <Alex0007lolpvp@gmail.com>",
  "license": "ISC"
}
