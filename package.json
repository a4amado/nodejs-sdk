{
  "name": "webdock",
  "version": "1.0.2",
  "description": "NodeJS SDK Library / Wrapper for the Webdock API",
  "scripts": {
    "release": "release-it",
    "build": "tsc",
    "generate-client": "node generate.js",
    "develop": "ts-node-dev --transpile-only --no-notify",
    "test": "jest",
    "prepublishOnly": "npm run generate-client && npm run build && cp -r ./dist/* . && rm -rf ./dist",
    "postpublish": "git clean -fd"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.8",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.5.1",
    "lodash": "^4.17.20",
    "openapi-typescript-codegen": "^0.6.0",
    "prettier": "^2.1.2",
    "release-it": "^14.2.1",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "webdock-io",
    "webdock"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Alex0007 <Alex0007lolpvp@gmail.com>",
  "license": "ISC"
}
